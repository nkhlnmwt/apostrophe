{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Run mocha",
      "type": "node",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "cwd": "${workspaceRoot}",
      "args": ["--compilers", "js:babel-core/register"],
      "runtimeExecutable": null,
      "env": { "NODE_ENV": "production"}
    }
  ]
}
